{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && flask --app api/index db upgrade && flask --app api/index seed all && python3 -m flask --app api/index run -p 5324",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"pnpm run flask-dev\"",
    "build": "npm install && pip3 install -r requirements.txt && pip3 install --upgrade psycopg2-binary && flask --app api/index db upgrade && flask --app api/index seed all",
    "start": "next start",
    "lint": "next lint"
  },
  "vercel": {
    "builds": [
      {
        "src": "next-app",
        "use": "@vercel/node"
      }
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@nextui-org/react": "^2.2.9",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "13.4.3",
    "next-themes": "^0.2.1",
    "postcss": "8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slick": "^0.30.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
  }
}
